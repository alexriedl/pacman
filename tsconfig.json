{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "es6",
    "target": "es6",
    "sourceMap": false,
    "outDir": "build",
    "baseUrl": "./src",
    "experimentalDecorators": true,
    "paths": {
      "sengine": ["sengine/src/index.ts"],
      "sengine/*": ["sengine/src/*"]
    }
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}
